{
  "name": "habahaba-affiliate",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "clear-pub": "rm -rfv public/assets/*.js public/assets/*.css",
    "sync-pub": "rsync -av --progress dist/ public/",
    "zip-pub": "rm -rfv habahaba-affiliate.zip && zip -rv habahaba-affiliate.zip public",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --fix",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "@types/popper.js": "^1.11.0",
    "@vueuse/core": "^11.1.0",
    "bootstrap": "^5.3.3",
    "chart.js": "^4.4.6",
    "jwt-decode": "^4.0.0",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "pinia": "^2.2.4",
    "sweetalert2": "^11.14.5",
    "vee-validate": "^4.14.6",
    "vue": "^3.5.12",
    "vue-router": "^4.4.5",
    "vue-sweetalert2": "^5.0.11"
  },
  "devDependencies": {
    "@tsconfig/node20": "^20.1.4",
    "@types/bootstrap": "^5.2.10",
    "@types/lodash": "^4.17.13",
    "@types/node": "^20.16.11",
    "@vitejs/plugin-vue": "^5.1.4",
    "@vitejs/plugin-vue-jsx": "^4.0.1",
    "@vue/eslint-config-prettier": "^10.0.0",
    "@vue/eslint-config-typescript": "^14.0.1",
    "@vue/tsconfig": "^0.5.1",
    "eslint": "^9.12.0",
    "eslint-plugin-vue": "^9.29.0",
    "npm-run-all2": "^6.2.3",
    "prettier": "^3.3.3",
    "typescript": "~5.5.4",
    "vite": "^5.4.8",
    "vite-plugin-vue-devtools": "^7.4.6",
    "vue-tsc": "^2.1.6"
  }
}
